<?xml version="1.0" encoding="utf-8"?>
<SarRuleConfig>
    <!-- restore sar value when condition not match, for wifi and cell sar value -->
    <restore>
        <!-- restore wifi nvram to default value -->
        <sar_wifi_24Ghz_restore value="17" />
        <sar_wifi_5Ghz_restore value="14" />
    </restore>

    <!-- is_use_precondition is used to control power dissipation, recommend is 1 -->
    <configs is_use_precondition="1" >
        <!-- value remark: 1-true, 0-false, NA-not care, empty string ""-NA -->
        <!-- wifi_band: 0-2.4GHz, 1-5GHz -->

        <!-- sub0 voice + wifi staion + near light-->
        <config>
            <is_sub0_voice value="1" />
            <is_wifi_connected value="1" />
            <is_near_light value="1" />
            <is_headset_insert value="0" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="10" />
        </config>
        <!-- sub0 voice + wifi softap + near light -->
        <config>
            <is_sub0_voice value="1" />
            <is_wifi_ap_opened value="1" />
            <is_near_light value="1" />
            <is_headset_insert value="0" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="10" />
        </config>
        <!-- sub1 voice + wifi staion + near light -->
        <config>
            <is_sub1_voice value="1" />
            <is_wifi_connected value="1" />
            <is_near_light value="1" />
            <is_headset_insert value="0" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="10" />
        </config>
        <!-- sub1 voice + wifi softap + near light -->
        <config>
            <is_sub1_voice value="1" />
            <is_wifi_ap_opened value="1" />
            <is_near_light value="1" />
            <is_headset_insert value="0" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="10" />
        </config>
        <!-- Sub0 Voice+nearlight off+WiFi station -->
        <config>
			<is_sub0_voice value="1" />
			<is_near_light value="0" />
            <is_wifi_connected value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub1 Voice+nearlight off+WiFi station -->
        <config>
			<is_sub1_voice value="1" />
			<is_near_light value="0" />
            <is_wifi_connected value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub0 Voice+nearlight off+WiFi AP -->
        <config>
			<is_sub0_voice value="1" />
			<is_near_light value="0" />
			<is_wifi_ap_opened value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub1 Voice+nearlight off+WiFi AP -->
        <config>
			<is_sub1_voice value="1" />
			<is_near_light value="0" />
			<is_wifi_ap_opened value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub0 Voicet+WiFi station+headset -->
        <config>
            <is_sub0_voice value="1" />
            <is_wifi_connected value="1" />
            <is_headset_insert value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub1 Voicet+WiFi station+headset -->
        <config>
            <is_sub1_voice value="1" />
            <is_wifi_connected value="1" />
            <is_headset_insert value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub0 Voicet+WiFi AP+headset -->
        <config>
            <is_sub0_voice value="1" />
			<is_wifi_ap_opened value="1" />
            <is_headset_insert value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>
        <!-- Sub1 Voicet+WiFi AP+headset -->
        <config>
            <is_sub1_voice value="1" />
			<is_wifi_ap_opened value="1" />
            <is_headset_insert value="1" />
            <sar_wifi_24Ghz_reduce value="17" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>		
		<!-- Sub0 Data+WiFi AP -->
        <config>
			<is_wifi_ap_opened value="1" />
			<is_sub0_data_connected value="1" />
			<is_headset_insert value="0" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="10" />
        </config>		
		<!-- Sub1 Data+WiFi AP -->
        <config>
			<is_wifi_ap_opened value="1" />
			<is_sub1_data_connected value="1" />
			<is_headset_insert value="0" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="10" />
        </config>		
		<!-- Sub0 Data+WiFi AP+headset -->
        <config>
			<is_wifi_ap_opened value="1" />
			<is_sub0_data_connected value="1" />
			<is_headset_insert value="1" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>		
		<!-- Sub1 Data+WiFi AP+headset -->
        <config>
			<is_wifi_ap_opened value="1" />
			<is_sub1_data_connected value="1" />
			<is_headset_insert value="1" />
            <sar_wifi_24Ghz_reduce value="14" />
            <sar_wifi_5GHz_reduce value="12" />
        </config>		
    </configs>
</SarRuleConfig>