<?xml version="1.0" encoding="utf-8"?>
<SarRuleConfig>
    <!-- restore sar value when condition not match, for wifi and cell sar value -->
    <restore>
	    <sar_cell_restore value="0" />
        <sar_wifi_restore value="17" />
        <sar_wifi_24GHz_restore value="17" />
        <sar_wifi_5GHz_restore value="17" />
    </restore>

    <!-- is_use_precondition is used to control power dissipation, recommend is 1 -->
    <configs is_use_precondition="0" >
        <!-- value remark: 1-true, 0-false, NA-not care, empty string ""-NA -->
        <!-- wifi_band: 0-2.4GHz, 1-5GHz -->
        <config>
		    <sim_type value="0" />
            <is_wifi_connected value="1" />
            <sar_wifi_24GHz_reduce value="10" />
            <sar_wifi_5GHz_reduce value="7" />
        </config>       
        <config>
		    <sim_type value="2" />
            <is_wifi_connected value="1" />
            <sar_wifi_24GHz_reduce value="10" />
            <sar_wifi_5GHz_reduce value="7" />
        </config>
    </configs>
</SarRuleConfig>

