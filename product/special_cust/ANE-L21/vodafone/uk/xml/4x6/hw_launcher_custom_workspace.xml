<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.huawei.android.launcher" version="1">
    <specialorder  launcher:op="screen+1" />
    <favorite
        launcher:packageName="com.android.gallery3d"
        launcher:className="com.huawei.gallery.app.GalleryMain"
        launcher:container="-100"
        launcher:screen="2"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="1"
        launcher:spanY="1" 
        launcher:op="insertinto"/>
    <favorite
        launcher:packageName="com.myvodafoneapp"
        launcher:className="com.myvodafoneapp.ui.activity.LaunchActivity"
        launcher:container="-100"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="5"
        launcher:spanX="1"
        launcher:spanY="1"
        launcher:op="insertinto" />
    <folder
        launcher:title="@string/folder_top_apps"
        launcher:container="-100"
        launcher:screen="2"
        launcher:x="3"
        launcher:y="2"
        launcher:op="insertinto" >
        <favorite
            launcher:packageName="com.facebook.katana"
            launcher:className="com.facebook.katana.LoginActivity" />
        <favorite
            launcher:packageName="com.instagram.android"
            launcher:className="com.instagram.android.activity.MainTabActivity" />
    </folder>
    <favorite
        launcher:packageName="com.vodafone.vodafone360updates"
        launcher:className="com.vodafone.vodafone360updates.AllServicesActivity"
        launcher:container="-100"
        launcher:screen="2"
        launcher:spanX="1"
        launcher:spanY="1"
        launcher:op="add" />
    <!--桌面小工具-->
    <appwidget
        launcher:packageName="com.vodafone.vodafone360updates"
        launcher:className="com.vodafone.smhs.SMHSDashboardProvider"
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="4"
        launcher:spanY="2"
        launcher:op="insertinto" />
</favorites>
